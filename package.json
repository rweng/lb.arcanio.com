{
  "name": "logbook",
  "private": true,
  "description": "Personal Logbook",
  "version": "0.0.1",
  "author": "Robin Wenglewski <robin@wenglewski.de>",
  "engines": {
    "node": ">=6.3 <7.0",
    "npm": ">= 3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rweng/logbook.git"
  },
  "bugs": {
    "url": "https://github.com/rweng/logbook/issues"
  },
  "scripts": {
    "start": "webpack-dev-server --config=webpack/dev.config.js --progress",
    "build": "rm -rf dist && npm i && NODE_ENV=production webpack --config webpack/prod.config.js --progress --profile --bail",
    "start.dist": "browser-sync start --port 8080 --serveStatic 'dist' --files 'dist'",
    "lint": "tsc && tslint '{src,test}/{,**/}*.ts'",
    "test": "true",
    "infrastructure": "cd infrastructure && terraform apply",
    "release": "npm run build && s3cmd sync --rr --delete-removed --add-header='Cache-Control:max-age=2592000,public' dist/ s3://lb.arcanio.com && s3cmd sync --add-header='Cache-Control:no-cache,public' dist/index.html s3://lb.arcanio.com/index.html"
  },
  "dependencies": {
    "@angular/common": "^2.2.0",
    "@angular/compiler": "^2.2.0",
    "@angular/core": "^2.2.0",
    "@angular/forms": "^2.2.0",
    "@angular/platform-browser": "^2.2.0",
    "@angular/platform-browser-dynamic": "^2.2.0",
    "@angular/router": "^3.0.0",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.6",
    "@rweng/ng2-markdown": "https://dl.dropboxusercontent.com/u/456244/npm/rweng-ng2-markdown-0.0.1.tgz",
    "@rweng/ng2-notifications": "https://dl.dropboxusercontent.com/u/456244/npm/rweng-ng2-notifications-0.0.1.tgz",
    "@rweng/store": "https://dl.dropboxusercontent.com/u/456244/npm/rweng-store-0.0.1.tgz",
    "@rweng/util": "https://dl.dropboxusercontent.com/u/456244/npm/rweng-util-0.0.1.tgz",
    "bootstrap": "^4.0.0-alpha.4",
    "debug": "^2.2.0",
    "font-awesome": "^4.6.3",
    "lodash": "^4.16.6",
    "offline-plugin": "^3.4.2",
    "pouchdb": "^6.0.6",
    "pouchdb-find": "^0.10.3",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.0-rc.3",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/debug": "0.0.29",
    "@types/lodash": "^4.14.38",
    "@types/node": "^6.0.46",
    "@types/pouchdb": "^5.4.28",
    "awesome-typescript-loader": "^2.1.1",
    "browser-sync": "^2.12.8",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.17.0",
    "json-loader": "^0.5.4",
    "memdown": "^1.2.2",
    "mocha": "^3.0.2",
    "mockery": "^1.7.0",
    "node-sass": "^3.10.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "sinon": "^2.0.0-pre.3",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-node": "^1.3.0",
    "tslint": "^3.14.0",
    "typescript": "^2.0.2",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-server": "2.1.0-beta.5",
    "webpack-merge": "^0.14.1"
  }
}
